    <link href='https://fonts.googleapis.com/css?family=Rock+Salt' rel='stylesheet' type='text/css'>
  </head>
  <body>
    <nav id="primary_nav_wrap">
      <h1>Del Mar Orchids</h1>
      <ul class="navbar">
        <li class="current-menu-item"><a href="/">HOME</a></li>
        <li class="gallery"><a href="/#gallery">GALLERY</a></li>
        <li class="care"><a href="/#care">ORCHID CARE</a>
          <ul>
            <li><a href="/care1">PHALAENOPSIS CULTURE</a></li>
            <li><a href="/care2">CATTLEYA CULTURE</a></li>
            <li><a href="/care3">DENDROBIUM CULTURE</a></li>
            <li><a href="/care4">ONCIDIUM CULTURE</a></li>
            <li><a href="/care5">PAPHIOPEDILUM CULTURE</a></li>
          </ul>
        </li>
        <li class="about"><a href="/#about">ABOUT US</a></li>
        <li class="contact"><a href="#contact">CONTACT US</a></li>
      </ul>
      <div class="social">
        <a href="https://www.facebook.com/delmarorchids/" target="_blank"><img src="../images/fb-512.png" width="20px" height="20px"></a>
        <a href="https://twitter.com/delmarorchids" target="_blank"><img src="../images/t-512.png" width="20px" height="20px"></a>
