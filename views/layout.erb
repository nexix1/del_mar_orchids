    <%= yield %>
    <section id="contact">
      <footer class="module content">
        <div class="container">
          <div class="addy">
            <p>4343 Carmel Valley Road, San Diego, CA 92130</p>
            <p>Phone: (858) 794-0774</p>
            <p>Email: delmarorchids@gmail.com</p>
          </div>
          <form method="post" action="/email_response"> 
            <input type="text" id="frombox" name="from" placeholder="from">
            <!-- <input type="post" id="tobox" name="to" placeholder="to"> -->
            <input type="post" id="subbox" name="subject" placeholder="subject">
            <textarea type="post" id="bodybox" name="body" placeholder="body"></textarea>
            <input type="submit" class="button">
          </form>
        </div>
      </footer>
    </section>
